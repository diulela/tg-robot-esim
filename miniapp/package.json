{
  "name": "tg-miniapp-esim",
  "version": "1.0.0",
  "description": "Telegram Mini App for eSIM E-commerce with Vue 3.0 + Vuetify",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "type-check": "vue-tsc --noEmit",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@mdi/font": "^7.4.0",
    "axios": "^1.6.0",
    "dayjs": "^1.11.0",
    "pinia": "^2.1.0",
    "qrcode": "^1.5.3",
    "vue": "^3.4.0",
    "vue-router": "^4.2.0",
    "vuetify": "^3.4.0"
  },
  "devDependencies": {
    "@types/qrcode": "^1.5.6",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "@vitejs/plugin-vue": "^4.5.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.0",
    "eslint": "^8.0.0",
    "eslint-plugin-vue": "^9.0.0",
    "prettier": "^3.0.0",
    "sass": "^1.69.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vite-plugin-vuetify": "^2.0.0",
    "vue-tsc": "^1.8.0"
  }
}
